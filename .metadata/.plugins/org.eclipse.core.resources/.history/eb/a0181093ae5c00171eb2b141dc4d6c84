package models;

import java.io.Serializable;

public class BeanComment implements Serializable{
	private static final long serialVersionUID = 1L;
	
	private int id;
	private int tweetId;
	private int userId;
	private String name;
	private String text;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	
}
