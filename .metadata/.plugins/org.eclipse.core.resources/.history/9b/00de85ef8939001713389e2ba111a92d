var APP = {
	init: function(){
		goTo("home");
		checkClick();
	}
}

function goTo(str){
	$('.mainCont').load(str, {"page": str}, function(){
		document.querySelector('#pageStyle').href = "css/" + str + ".css";
	});
}

function checkClick(){
	document.querySelector('.register').onclick = function(){
		goTo("register");
	}
	document.querySelector('.login').onclick = function(){
		goTo("login");	
	} 
}

APP.init();

